{
  "name": "willhoffmann/domuserp-module",
  "description": "Zend Framework 3 Module that provides Domus ERP API functionality.",
  "type": "library",
  "license": "MIT",
  "keywords": ["domus erp", "module", "zf3", "rest"],
  "authors": [
    {
      "name": "William Hoffmann",
      "email": "williamhoffmann@outlook.com",
      "role": "Developer"
    }
  ],
  "homepage": "https://github.com/willhoffmann/DomusErpModule",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^7.1",
    "willhoffmann/domuserp-php": "^1.0",
    "zendframework/zend-modulemanager": "^2.7",
    "zendframework/zend-mvc": "^3.1",
    "zendframework/zend-servicemanager": "^3.3"
  },
  "autoload": {
    "psr-4": {
      "DomusErpModule\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DomusErpModuleTest\\": "test/"
    }
  },
  "scripts": {
    "cs-check": "phpcs",
    "cs-fix": "phpcbf"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "zf": {
      "module": "DomusErpModule"
    }
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.2"
  }
}
